# References:
# https://people.sc.fsu.edu/~jburkardt/data/obj/cube.obj
# https://grabcad.com/tutorials/create-an-obj-cube-with-your-favorite-images

mtllib skybox.mtl

o skybox

v -1.0 -1.0 -1.0
v 1.0 -1.0 -1.0
v 1.0 1.0 -1.0
v -1.0 1.0 -1.0
v -1.0 -1.0 1.0
v 1.0 -1.0 1.0
v 1.0 1.0 1.0
v -1.0 1.0 1.0

vn 0 -1 0.5
vn 0 1 0.5
vn -1 0 0.5
vn 1 0 0
vn 0 0 1
vn 0 0 -1

vt 0 0
vt 1 0
vt 0 1
vt 1 1

usemtl front
f 1/1/1 2/2/1 6/4/1
f 1/1/1 6/4/1 5/3/1
usemtl back
f 3/1/2 4/2/2 8/4/2
f 3/1/2 8/4/2 7/3/2
usemtl left
f 4/1/3 1/2/3 5/4/3
f 4/1/3 5/4/3 8/3/3
usemtl right
f 2/1/4 3/2/4 7/4/4
f 2/1/4 7/4/4 6/3/4
usemtl top
f 5/1/5 6/2/5 7/4/5
f 5/1/5 7/4/5 8/3/5
usemtl bottom
f 4/1/6 3/2/6 2/4/6
f 4/1/6 2/4/6 1/3/6